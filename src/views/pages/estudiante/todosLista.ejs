<link rel="stylesheet" href="/css/bootstrap.css">
<link rel="stylesheet" href="/css/bootstrap.min.css">
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>

</head>

<body>
  <% var i = 1 %>
  <!-- ///////////////////////////////////////////////////////7 -->
  <% cursos.forEach(function(curso) { %>
    <% i = 1 %>
  <div class="container p-0 my-3"> 
    <h3 id="cursoClave">
      Curso <%= curso.grado %>° <%= curso.division %>°
    </h3> 
    <div class="">
      <div>

        <hr class="mb-0">
      </div>
      <table id="estudiantes" class="table table-striped table-bordered table-sm">
        <thead class="">
          <tr>
            <th class="">N°</th>
            <th class="col-md-2">Estudiante</th>
            <th class="">DNI</th>
            <th class="col-md-1"><small>Fecha:</small></th>
            <th class="col-md-1"><small>Fecha:</small></th>
            <th class="col-md-1"><small>Fecha:</small></th>
            <th class="col-md-1"><small>Fecha:</small></th>
            <th class="col-md-1"><small>Fecha:</small></th>
            <th class="col-md-1"><small>Fecha:</small></th>
          </tr>
        </thead>
        <tbody class="tbl_user_data">
          <% estudiantesArray.forEach(function(registro) { %>
            <% if( registro.estudianteCurso.curso == curso.idCurso ){%>
          <tr class="p-0">
           <td>
            <%= i++ %>
           </td>
            <td>
              <%= registro.estudiante.apellido %>, <%= registro.estudiante.nombre %>
            </td>
            <td>
              <%= registro.estudiante.dni %>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
          </tr>
          <!-- Modal -->
          <% } %>
          <% }) %> 
        </tbody>

      </table>
    </div>
  </div>
  <div style="page-break-after: always;"></div>
  <% }) %> 
  <hr>
